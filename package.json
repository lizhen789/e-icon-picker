{
  "name": "e-icon-picker",
  "version": "2.0.0-alpha.1",
  "description": "element-ui和font-awesome图标选择器",
  "keywords": [
    "vue",
    "icon-picker",
    "e-icon-picker",
    "iconPicker",
    "eiconPicker",
    "icon picker",
    "icon"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm-bundler.js",
  "unpkg": "lib/index.global.prod.js",
  "jsdelivr": "lib/index.global.prod.js",
  "style": "lib/index.css",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cnovel/e-icon-picker"
  },
  "author": "cnovel.club",
  "license": "MIT",
  "bugs": {
    "url": "https://gitee.com/cnovel/e-icon-picker/issues"
  },
  "homepage": "https://doc.icon.cnovel.club",
  "files": [
    "lib",
    "src",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean:lib && yarn build:lib && yarn build:theme",
    "build:lib": "rollup --config ./scripts/rollup.config.js",
    "build:theme": "webpack --config ./scripts/webapck.config.js && cp-cli scripts/dist/index.css lib/index.css && cp-cli scripts/dist/fonts lib/fonts && rimraf scripts/dist",
    "clean:lib": "rimraf lib && rimraf scripts/dist"
  },
  "dependencies": {
    "element-plus": "^1.0.2-beta.40",
    "vue": "^3.0.11"
  },
  "peerDependencies": {
    "element-plus": "^1.0.2-beta.40",
    "font-awesome": "^4.7.0",
    "svg-sprite-loader": "^5.0.0",
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/compiler-sfc": "^3.0.7",
    "autoprefixer": "^10.2.5",
    "babel-plugin-component": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cp-cli": "^2.0.0",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
