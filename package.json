    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.4",
    "postcss-loader": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.32.11",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "tslib": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^4.6.0"
