{
  "name": "e-icon-picker",
  "version": "2.0.4",
  "description": "element-ui和font-awesome图标选择器",
  "keywords": [
    "vue",
    "icon-picker",
    "e-icon-picker",
    "iconPicker",
    "eiconPicker",
    "icon picker",
    "icon"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm-bundler.js",
  "unpkg": "lib/index.global.prod.js",
  "jsdelivr": "lib/index.global.prod.js",
  "style": "lib/index.css",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cnovel/e-icon-picker"
  },
  "author": "cnovel.club",
  "license": "MIT",
  "bugs": {
    "url": "https://gitee.com/cnovel/e-icon-picker/issues"
  },
  "homepage": "https://e-icon-picker.cnovel.club",
  "files": [
    "lib",
    "src",
    "types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:font-css": "yarn all && cp-cli scripts/fontawesome lib/fontawesome && cp-cli scripts/ele lib/ele && yarn clean:font-css",
    "clean:font-css": "rimraf scripts/fontawesome && rimraf scripts/ele",
    "all": "yarn fontawesome-5.x.x && yarn fontawesome-5.x.x-pro && yarn fontawesome-4.7.0 && yarn ele-plus",
    "fontawesome-5.x.x": "node scripts/fontawesome-5.x.x.js",
    "fontawesome-5.x.x-pro": "node scripts/fontawesome-5.x.x-pro.js",
    "fontawesome-4.7.0": "node scripts/font-awesome.v4.7.0.js",
    "ele-plus": "node scripts/element-plus.js",
    "build": "yarn clean:lib && yarn build:lib && yarn build:theme && yarn build:font-css",
    "build:lib": "rollup --config ./scripts/rollup.config.js",
    "build:theme": "webpack --config ./scripts/webapck.config.js && cp-cli scripts/dist/index.css lib/index.css && cp-cli scripts/dist/fonts lib/fonts && rimraf scripts/dist",
    "clean:lib": "rimraf lib && rimraf scripts/dist && yarn clean:font-css",
    "publish": "npm publish --tag next"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "element-plus": "^1.0.2-beta.42",
    "font-awesome": "^4.7.0",
    "svg-sprite-loader": "^6.0.5",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "element-plus": "^1.0.2-beta.42",
    "font-awesome": "^4.7.0",
    "svg-sprite-loader": "^6.0.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/compiler-sfc": "^3.1.4",
    "autoprefixer": "^10.2.6",
    "babel-plugin-component": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cp-cli": "^2.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano": "^5.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.0.0",
    "mitt": "^3.0.0",
    "normalize-wheel": "^1.0.1",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "tslib": "^2.3.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
